<SCRIPT language=javascript>
var fromSizeChanged = 0;
// superPID id: 3175072 //

//  contentPopup tab variables  //
var contentPopupSizeChartUrl = null;

function getEnlargeUrl() {
	return "productId=3175072";
}

function getSizeChartUrl() {
	return contentPopupSizeChartUrl;
}

function getPopupUrl() {
	var temp = getEnlargeUrl();
	if( getSizeChartUrl() != null ) {
		temp += '&' + getSizeChartUrl();
	}
	return temp;
}
//  End of customPopup tab variables  //

function showEnhanced(prodIndex) {
	omniImpl('Enlargeimage');
	remote=open("/contentPopup/index.jsp?ab=Enlargeimage&" + getPopupUrl(), "largeImage", "width=805,height=617,left=0,top=0");
}

