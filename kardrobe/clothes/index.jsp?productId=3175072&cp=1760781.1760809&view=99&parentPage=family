function sizeChanged(indexSelected,shirtIndexSelected,alphaIndexSelected){
	initVar = 'Reg';
	if(indexSelected == 0){
		initVar = 'Init';
		shirtIndexSelected= 0;
		alphaIndexSelected =0;
		currentSize = 0;
	} else{
		currentSize = indexSelected - 1;
	}
	changeShirtforSize(indexSelected,shirtIndexSelected,alphaIndexSelected);
	fromSizeChanged = '1';
	//omniEngagementImpressions("event44", "Product Feature: Size");
	updateShirtViewOnClick(shirtIndexSelected,alphaIndexSelected);
}

//function showSaleWithColor added for adding sale note in the color dropdown
function showSaleWithColor() {
	sizeDropDown = eval('document.color_form.sizeField');
	colorDropDown = eval('document.color_form.shirt_color');
	if(colorDropDown.type == "select-one" && sizeDropDown.type == "select-one")  {
	    var selectedIndexOfSize= sizeDropDown.selectedIndex;
		var pidSkuArrayForSize = new Array();
		if(selectedIndexOfSize != 0) {
			pidSkuArrayForSize = eval('colorPidSkuArReg' + childProdIndex + (selectedIndexOfSize- 1));
		} else {
			pidSkuArrayForSize = eval('colorPidSkuArReg' + childProdIndex + (selectedIndexOfSize));
		}	
	    var isAllOnSale = "true";
	    var colorCounter= 0;
	    var saleCounter=0;

	    //loop to determine if all skus are on sale
	    //count skus on sale
		itemMap = eval("itemMap" + childProdIndex);	
	    for(; colorCounter < colorDropDown.options.length && colorCounter < pidSkuArrayForSize.length;colorCounter++){
	      var pidSkuArray = pidSkuArrayForSize[colorCounter].split("|");
	      var curSku = pidSkuArray[1];

			for(var counter = 0; counter < itemMap.length; counter++) {
				if(itemMap[counter].skuid == curSku) {
					if(parseFloat(itemMap[counter].currentprice) < parseFloat(itemMap[counter].baseprice)) {
						saleCounter=saleCounter + 1;
					}
				}
			}
	    }

	    //if no. of skus on sale < no. of skus in color drop-down
	    //then display "sale" in color drop-down
	    if (saleCounter < colorDropDown.options.length) {
			var colorCounter= 0;
			for(; colorCounter < colorDropDown.options.length && colorCounter < pidSkuArrayForSize.length;colorCounter++) {
				var pidSkuArray = pidSkuArrayForSize[colorCounter].split("|");
				var curSku = pidSkuArray[1];

		        for(var counter = 0; counter < itemMap.length; counter++) {
					if(itemMap[counter].skuid == curSku) {
						if(parseFloat(itemMap[counter].currentprice) < parseFloat(itemMap[counter].baseprice)) {
							colorDropDown.options[colorCounter].text = colorDropDown.options[colorCounter].text + " - SALE";
						}
						break;
					}
		        }
			}
	    }
	}
}

var persistentUpdateShirtViewNumber=0;
var persistentUpdateShirtViewSelectedAlphaIndex=0;
var atlView = false;

function updateShirtViewOnClick(number, selectedAlphaIndex) {
	persistentUpdateShirtViewNumber = number;	
	persistentUpdateShirtViewSelectedAlphaIndex = selectedAlphaIndex;
	updateShirtView(number, selectedAlphaIndex, true);
	atlView = false;
}

function updateShirtViewOnMouseOver(number, selectedAlphaIndex) {

}

function updateShirtViewOnMouseOut() {

}

function updateShirtViewOnClickView(REGPath) {
	persistentAltView = REGPath;
	updateShirtViewAlt(REGPath);
	atlView = true;
}

function updateShirtViewOnMouseOverView(REGPath) {

}

function updateShirtViewOnMouseOutView() {

}

function updateShirtViewAlt(REGPath) {
	thisProdShotView = eval('window.document.itempic');
	if (REGPath != "") {
		thisProdShotView.src = REGPath;
	}
}

function updateShirtView(number,selectedAlphaIndex, bUpdateDropdown){
	var alphaIndex = 0;
	if(selectedAlphaIndex){		
		alphaIndex = selectedAlphaIndex;
	}
	//alert('initVar=' + initVar);
	var alphaImgAr = eval('alphaImg' + initVar + childProdIndex + currentSize + '_' + number);
	var mcolorImgAr = eval('mcolorImg' + initVar + childProdIndex + currentSize + '_' + number);
	
	var layoutAr =  eval('layoutId' + childProdIndex + currentSize);
	currentAlpha = alphaImgAr[alphaIndex].webcode;
	if(mcolorImgAr[0]) {
	currentHex =  mcolorImgAr[0].hexKeyValue;
	}

	currentLayout = layoutAr[number];
	currentShirtNumber = number;
	if( document.color_form.shirt_color && ((bUpdateDropdown == null) || (bUpdateDropdown == true)) ) {
		document.color_form.shirt_color.options[currentShirtNumber].selected = true;
	} 
	var hasName = eval('hasNameLayout' + childProdIndex + currentSize);
	
	var hasNameL = hasName[number];

	if(hasNameL == 'false' && document.getElementById('monoAndQuant'))
		{
			document.getElementById('monoAndQuant').style.visibility = 'hidden';
			document.getElementById('monoAndQuant').style.display = 'none';
			document.getElementById('qty4').style.visibility = 'visible';
			document.getElementById('qty4').style.display = 'block';
		} else if(hasNameL == 'true') {
			document.getElementById('monoAndQuant').style.visibility = 'visible';
			document.getElementById('monoAndQuant').style.display = 'block';
			document.getElementById('qty4').style.visibility = 'hidden';
			document.getElementById('qty4').style.display = 'none';
		}
	changeTextFontforShirt(number,selectedAlphaIndex);
	updatechains();
	
}
var altEnhImg = '';
var fromPreview = 'N';
var nameString = 'YOUR NAME';
function updatechains(){
	//alert('currentHex' + currentHex);
	var textURLValue = '';
	var textURLValueName = '';
	imgWidth = eval("imgWidth" + childProdIndex);
	
	var displaySizeShirtURL = '&set=key[displaysize],value[' + imgWidth[0] + ']';
	var filterChainAr = eval('ivFilterChainFile' + initVar + childProdIndex + currentSize + '_' + currentShirtNumber );
	var currentFont = '';
	var str = 'YOUR NAME';

	var hasName = eval('hasNameLayout' + childProdIndex + currentSize);
	
	var hasNameL = hasName[currentShirtNumber];
	if(hasNameL == undefined) {
		hasNameL = hasName[0];		
	}
	var baseURLMain = ivFilterChainURL;
	var endURLMain = filterChainAr[0];
	var alphaURLValuePony='';
	var modeURLValue = '';
	var defaultCmodeCode = eval('defaultCmode' + childProdIndex);
	if(hasNameL == 'true') {
		str = nameString;
		currentFont = document.color_form.fontType.value;
		

		if(fromPreview == 'Y' ){
		str = document.color_form.monogramName.value;

		document.getElementById("errorMessagingDiv").innerHTML='';
		if(str == ''|| str.toUpperCase() =='YOUR NAME') {

				

			displayError('Please enter your name', {obj:'errorContainer'});
			
	    
							

				
				fromPreview = 'N';
				
				
				return;
			
		} 	
		  else{
                var eVar47Value = "Product Feature: Monogram: Name: "+str;
         	    //omniEngagementImpressions("event44", eVar47Value);

				window.scrollTo(100,220);
			}			
		fromPreview = 'N';	
		nameString = str; //Entered Name
	} 

		
		cmodeImagingTagName = eval("cmodeImagingTagName" + childProdIndex);		
		if(cmodeImagingTagName.value == null) {			
			cmodeImagingTagName = 'decoration';
		}
		modeURLValueName = 'set=key[' + cmodeImagingTagName + '],value[' + currentFont + ']';
		textURLValueName = '&set=key[' + text1ImagingTag + '],value[' + escape(str) + ']&'+ currentHex;
	}
	
	/*The LP Chain image for main image 360 X 480 */
	cmodeImagingTag = eval("cmodeImagingTag" + childProdIndex);
	modeURLValue = 'set=key[' + cmodeImagingTag + '],value[' + currentAlpha + ']';
	alphaURLValuePony = '&set=key[ponyimage],value[' + currentAlpha + ']';
	var finalURLMain = baseURLMain + modeURLValue + textURLValue + displaySizeShirtURL  + endURLMain;
	//this is just setting the hidden filed altEnhImg to the last clicked 360 X 480 image. 
	document.color_form.altEnhImg.value = finalURLMain.replace(/\?/g,'%3F');
	document.itempic.src = finalURLMain;	

	/*The LP Chain image for left flag image 163 X 103 */
	var displaySizeRLURL = '&set=key[displaysize],value[' + imgWidth[1]  + ']';
	var baseURLEmb = ivFilterChainURL;
	var endURLEmb = filterChainAr[1];
	var finalURLLeft = baseURLMain + modeURLValue + textURLValue  + displaySizeRLURL + endURLEmb
	document.alphashirt.src = finalURLLeft;	
	
	/*The LP Chain image for right flag image 163 X 103 */
	var displaySizePonyURL = '&set=key[displaysize],value[' + imgWidth[1]  + ']';
	var endURLAlpha =filterChainAr[2];
	var finalURLRight = '';
	if(hasNameL == 'true') {
		finalURLRight  = baseURLMain + modeURLValueName + textURLValueName  + displaySizePonyURL + endURLAlpha;	
	}else{
		finalURLRight  = baseURLMain + modeURLValue + textURLValue  + displaySizePonyURL + endURLAlpha;
	}	
	document.ponyshot.src = finalURLRight;	
}

function changeShirtforSize(sizeNumber,selectedShirtIndex,selectedAlphaIndex)
{
	var shirtImgCode = '<div>';
	var shirtImgAr = eval('shirtImg' + initVar + childProdIndex + currentSize);
	var shirtAltAr = eval('shirtColor' + initVar + childProdIndex + currentSize);
	
	for( i = 0 ; i < shirtImgAr.length ; i++ )
	{
		if( (i != 0) && (i % 6 == 0) )
		{
			shirtImgCode = shirtImgCode + '</div><div>';
		}

		shirtImgCode = shirtImgCode
			+ '<a href="javascript:updateShirtViewOnClick(' + i + ',&quot;&quot;)"'
				+ 'onMouseOver="updateShirtViewOnMouseOver(' + i + ',&quot;&quot;);"'
				+ 'onMouseOut="updateShirtViewOnMouseOut();">'
					+ '<img name=pony' + i
						+ ' src= ' + shirtImgAr[i]
						+ ' border=0 width=50 height=50 alt = \'' + shirtAltAr[i] + '\' title=\'' + shirtAltAr[i] + '\' >'
			+ '</a>';
	}

	document.getElementById("shirt_div").innerHTML = shirtImgCode + '</div>';
	var shirtIndex = 0;
	if( selectedShirtIndex )
	{
		shirtIndex = selectedShirtIndex;
	}
	
	/*Building the AVAILABLE COLORS dropdown dynamically. */
	var fieldName;
	if(document.color_form.shirt_color) {
		fieldName = eval('document.color_form.shirt_color');
	}
	
	var options = fieldName.options;
	options.length=0;
	var counter = 0;
	
	for (var i = 0; i < shirtAltAr.length; i++)
	{
		var newOpt = document.createElement("OPTION");
		newOpt.value = shirtAltAr[i];
		newOpt.text = shirtAltAr[i];
		options[counter] = newOpt;
		counter++;
	}
	options[0].selected = true;
	
	/* Displaying the selected child PID's price */
	document.getElementById("Step5or6Quantity1").innerHTML = eval('productPrice' + childProdIndex);
}

function initialLoad(){

	if( 0 == 0){
		initVar='Init';
	} else {
		initVar='Reg';
	}
	if(true) {
	changeShirtforSize(0,0,0);
	updateShirtView(0,0);
	}
}
function updateShirtViewOnClickFont(number, isColor, selectedAlphaIndex)
{
	
	persistentUpdateShirtViewNumber[isColor]                = number;
	persistentUpdateShirtViewSelectedAlphaIndex[isColor]    = selectedAlphaIndex;
	updateShirtView(number,isColor,selectedAlphaIndex,false);
	
}

function changeTextFontforShirt(shirtnumber,selectedAlphaIndex)
{
	var fontImgCode = '<ul class="font_list">';
	var fontImgAr = eval('fontImg' + initVar + childProdIndex + currentSize + '_' + shirtnumber);
	var fontAltAr = eval('fontAlt' + initVar + childProdIndex + currentSize + '_' +  shirtnumber);
	var fontURLAr = eval('fontURL' + initVar + childProdIndex + currentSize + '_' + shirtnumber);
	
	for( i = 0 ; i < fontImgAr.length ; i++)
	{
		if( (i == 0) || (i % 4 == 0) )
		{
			if (fontImgAr.length == 1){
				fontImgCode = fontImgCode + '<li style="background-image: url(' + fontURLAr[i] +');" class="first single" >';
			}else{
				fontImgCode = fontImgCode + '<li style="background-image: url(' + fontURLAr[i] +');" class="first" >';	
			}
		} else {
			fontImgCode = fontImgCode + '<li style="background-image: url(' + fontURLAr[i] +');" >';
		}

	    /* Building the fonts radio buttons. */
		fontImgCode = fontImgCode

			+ '<input type="radio" name="fontType" value="' + fontImgAr[i].webcode + '" id="fontTypeId' + i + '" onClick="updateShirtViewOnClickFont(' + i + ',\'font\',\'\')" title="' + fontAltAr[i] + '" />'
			+ '</li>';			
	}

	//document.getElementById("font_div").innerHTML = fontImgCode + '</ul>';
	//document.color_form.fontType[0].checked=true;
    
	
	if(fontImgAr.length == 1)
	{//style="margin-top: 20px; margin-bottom: 8px;padding-left:40%";
		document.getElementById("font_div").innerHTML = fontImgCode + '</ul>';
		document.color_form.fontType.checked=true;
	}else if(fontImgAr.length == 2)
	{
		document.getElementById("font_div").innerHTML = fontImgCode + '</ul>';
		document.color_form.fontType[0].checked=true;
	}else if(fontImgAr.length != 0)
	{
		document.getElementById("font_div").innerHTML = fontImgCode + '</ul>';
		document.color_form.fontType[0].checked=true;
	}	
	
	var fontIndex = 0;
	if(selectedAlphaIndex)
	{
		fontIndex = selectedAlphaIndex;
	}		
}

function monogramNameToUpperCase(){
	document.color_form.monogramName.value=document.color_form.monogramName.value.toUpperCase();
}

function onBlurCheckTextBox(){

	if(document.color_form.monogramName.value == "")
	{
		document.color_form.monogramName.value = "YOUR NAME";
		document.getElementById('char_count').innerHTML="";
	 }
    
}

function updateNameVariableonClick(value){
	 var     eVar37Variable = "Product Feature: Monogram: Name: ";
	 eVar37Variable = eVar37Variable + value;
	 //omniEngagementImpressions("event44", eVar37Variable);
}

