var persistentIndex = 0;
var persistentFlagIndex = 0;
var persistentSizeIndex = 0;
var persistentColorIndex = 0;

function setChildProdIndexOnMouseClick(index,sizeDropDown,flagIndex) {
	persistentIndex = index;
	persistentFlagIndex = flagIndex;
	persistentSizeIndex = 0;
	persistentColorIndex = 0;
   //omniEngagementImpressions("event44", "Product Feature: Flag");
	setChildProdIndex(index,sizeDropDown,flagIndex,true);
	atlView = false;
}

function setChildProdIndexOnMouseOver(index,sizeDropDown,flagIndex) {
	
}

function setChildProdIndexOnMouseOut(sizeDropDown) {
	
}

function setChildProdIndex(index,sizeDropDown,flagIndex,updateDropDown) { 	
	/*resetting the sizes based on the country selected.*/
	for(var x=0;x<sizeDropDown.length;x++) {
		sizeDropDown.options[x] = null;
	}
	
	/* Update the Available Flags drop down accordingly. (only on click) */
	if( document.color_form.countryField && ((updateDropDown == null) || (updateDropDown == true))){
		document.color_form.countryField.options[flagIndex].selected = true;
	}
		
	sizeDropDown.length = 0;
	sizeDropDown.options[0] = new Option('Select Size','Select Size');
	sizeDropDown.options[0].selected = true;
	
	//we need to reset the currentSize var to 0 
	currentSize=0;
	
	childProdIndex = index;	
	currentChildProductId = childProductId[index];
	
	skuPrice = eval('skuPrice' + index);
	for(var t=0; t< skuPrice.length;t++) {
		sizeDropDown.options[t+1] = new Option(skuPrice[t],skuPrice[t]);
	}	
	
	initialLoad();
	document.getElementById("Step5or6Quantity1").innerHTML = eval('productPrice' + index);
}
