var submitProc = "";

function submit(submitProcess){
	submitProc = submitProcess;	
	var selectedIndex = document.color_form.sizeField.options.selectedIndex;

	var pidSkuAr = eval('colorPidSkuArReg' + childProdIndex + currentSize);
	var selectedColorIndex = document.color_form.shirt_color.options.selectedIndex;

	tcField = eval('document.color_form.featureValue');

	if(selectedIndex == 0)	{
		if(document.getElementById('showCartMessages'))
		{
				document.getElementById('showCartMessages').style.visibility = 'hidden';
				document.getElementById('showCartMessages').style.display = 'none';
		}
		
			document.getElementById('selectSizeMinicart').style.visibility = 'visible';
			document.getElementById('selectSizeMinicart').style.display = 'block';
		
			return;
	} else {
		document.getElementById("errorMessagingDiv").innerHTML = '';
	}
	document.color_form.callAddToCart.value = 'true';	
	document.color_form.prod_0.value = pidSkuAr[selectedColorIndex];
	document.color_form.layoutId.value = currentLayout;
	
	document.color_form.callAddToCart.value = 'true';
	document.color_form.action = 'polo_cyoFlagHandler.jsp?productId=12991193';
	
	for (i=0; i < document.color_form.elements.length; i++)
	{
		qtyField = document.color_form.elements[i];
		nameStr = qtyField.name;
		if (nameStr.match("qty"))
		{
			prodField = eval('document.color_form.prod_0');
			if(prodField != null && prodField != '' && prodField.value != null && prodField.value != '')
			{
		
				prodSplit = prodField.value.split("|");
				if(prodSplit[0] != null && prodSplit[0] != '' && prodSplit[1] != null && prodSplit[1] != '')
				{
					loadNewCartProducts(prodSplit[0],prodSplit[1]);
				}
			}
		}
	}	

	var nameEntered = document.color_form.monogramName.value;

	if(nameEntered != '' && nameEntered != 'YOUR NAME'){ 		
				ajaxBadWordCheck(nameEntered);

	}
	else{
		canAddItem();
	}

}


Array.prototype.contains = function (element) {
	for (var i = 0; i < this.length; i++) {
		if (this[i] == element) {
			return true;
		}
	}
	return false;
}

function ajaxBadWordCheck(processWord) {

	var word = processWord.toLowerCase();
	if(!supportedBrowser) {
		if(!(goodWords.contains(word)) && badWords.contains(word)) {
				displayError('We\'re sorry, but you have entered a restricted name or word. Please try again, or contact customer service for more information.', {obj:'errorContainer'});		
		}
		else {			
			canAddItem();
		}
	}
	else {		
		var pars = "textValue="+processWord;
		var url = "/product/custom_badWordCheck.jsp";
		var myAjax = new Ajax.Request( url, { method: 'post', parameters: pars, onComplete: handleBadWordChange }); 	
	}
}

function handleBadWordChange(req) {
	if(req.status == 200) {
		var txt = req.responseText;
		var hasBadWord = getXmlValue(txt, 'hasBadWord');
		if(hasBadWord == 'true') {			
			displayError('We\'re sorry, but you have entered a restricted name or word. Please try again, or contact customer service for more information.', {obj:'errorContainer'});
		}
		else{			
			canAddItem(); //add the item to cart since text entered is good.
		}
	}
	else {
		alert("Unable to retrieve a response from the server.");
	}	
}


function canAddItem()
{
	if (submitProc == "wishlist")
		{
			document.color_form.showMy.value = "wishlist";
			document.color_form.wlName.value = "wlName";
			document.color_form.secondaryAction.value = "customSkuAddToWish";
		}
		else
		{
			document.color_form.showMy.value = "";
			document.color_form.wlName.value = "";
		}
	
		if(!(submitProc == "wishlist"))
		{
						
				document.getElementById('selectSizeMinicart').style.visibility = 'hidden';
				document.getElementById('selectSizeMinicart').style.display = 'none';			
				ajaxAddToCart(document.color_form,'poloCustomFlagOutfit');			
			
		}
		else
		{
			document.color_form.submit();
		}

	
}


function showRLstyleGuide(pid)
{
	showCustomPopUp( '/contentPopup/index.jsp?' + getPopupUrl() + '&showStyleGuide', 'largeImage', 'width=805,height=617,left=0,top=0');
}

