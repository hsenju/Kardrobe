<script type="text/javascript">
jQuery.readyList.unshift(function(){

jQuery(unescape("div.com-amazon-webstore-ProductItemNumber-2%20dl.attributeList%20dt.itemNo")).each(function(){
    jQuery(this).find("*").add(this).contents().filter(function(){
        return (this.nodeType == 3) && (!/^\s*$/.test(this.nodeValue)) && (this.parentNode.tagName != "SCRIPT");
    }).each(function(){
        this.nodeValue = this.nodeValue.replace(unescape("Item%20No."), unescape("Style%20#"));
    });
});

});
