jQuery.readyList.unshift(function(){

jQuery(unescape("div..starter-amazon-com-RecentHistoryWidget-2%20.recentlyViewed%20.productDetails%20dl.priceBlock%20dt.listPrice")).each(function(){
    jQuery(this).find("*").add(this).contents().filter(function(){
        return (this.nodeType == 3) && (!/^\s*$/.test(this.nodeValue)) && (this.parentNode.tagName != "SCRIPT");
    }).each(function(){
        this.nodeValue = this.nodeValue.replace(unescape("Orig."), unescape("Was"));
    });
});

});
